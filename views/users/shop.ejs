      // Get all elements with the class 'submit-btn'
var submitButtons = document.querySelectorAll('.submit-btn');

// Loop through each submit button and add a click event listener
submitButtons.forEach(function(button) {
    button.addEventListener('click', function(event) {
        event.preventDefault(); // Prevent the default action (following the link)

        // Get the ID of the form associated with this button
        var productId = button.getAttribute('id').split('_')[1]; // Extract the product ID from the button ID
        var formId = 'myForm_' + productId; // Form ID based on the product ID

        // Submit the corresponding form
        document.getElementById(formId).submit();
    });
});



