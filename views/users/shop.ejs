    // Add click event listener to the document body and delegate it to .addToWishlistButton elements
    document.body.addEventListener('click', function(event) {
        if (event.target.matches('.addToWishlistButton')) {
            event.preventDefault(); // Prevent the default link behavior
            console.log("clicked");

            // Extract the product ID from the data attribute
            var productId = event.target.getAttribute('data-product-id');

            // Check if a product ID is found
            if (productId) {
                console.log("prodct Id",productId);
                // Prepare the data to be sent in the fetch request
                var data = {
                    productId: productId
                };

                // Send a POST request using Fetch
                fetch('/product/wishlist/add', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify(data)
                })
.then(response => {
    if (response.ok) {
        // Handle successful response
        return response.json();
    } else {
        // Handle error response
        throw new Error('Failed to add product to wishlist. Please try again.');
    }
})
