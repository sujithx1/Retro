    <script>
        console.log(`orderId <%= orderIdd %>`);
        let orderIdd=`<%= orderIdd %>`
        
        function confirmStatusChange(orderId, newStatus) {
    Swal.fire({
        title: 'Are you sure?',
        text: `You are about to change the status to ${newStatus}.`,
        icon: 'warning',
        showCancelButton: true,
        confirmButtonText: 'Yes, change it!',
        cancelButtonText: 'No, keep it'
    }).then((result) => {
        if (result.isConfirmed) {
            // Perform the status update
            fetch(`/admin/order/detail/OrderStatus/${orderId}`, {
                method: 'PUT',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({ status: newStatus ,orderIdd:orderIdd})
            })
            .then(response => response.json())
            .then(data => {
                if (data.success) {
                    Swal.fire(
                        'Updated!',
                        `Order status has been updated to ${newStatus}.`,
                        'success'
                    ).then(() => {
                        // Optionally reload the page or update the UI
                        location.reload();
                    });
                } else {
                    Swal.fire(
                        'Error!',
                        data.message||'There was an error updating the order status.',
                        'error'
                    );
                }
            })
            .catch(error => {
                Swal.fire(
                    'Error!',
                    'There was an error updating the order status.',
                    'error'
                );
            });
        }
    });
}


